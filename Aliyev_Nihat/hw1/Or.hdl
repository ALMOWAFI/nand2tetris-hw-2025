/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(a=a, out=notA);
    Not(a=b, out=notB);
    Nand(a=notA, b=notB, out=out);
}